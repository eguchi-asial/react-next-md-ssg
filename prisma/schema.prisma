generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model categories {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(255)
}

model markdowns {
  id       Int    @id @default(autoincrement())
  markdown String @db.LongText
  slug     String @db.VarChar(255)
  title    String @db.VarChar(255)
}

model d472dad6_d6ae_51e0_9240_11786e164ce0_20230822005200_vrepl {
  id          Int    @id @default(autoincrement())
  markdown    String @db.LongText
  category_id Int
  slug        String @db.VarChar(255)
  title       String @db.VarChar(255)

  @@index([category_id], map: "category_id_idx")
  @@map("_d472dad6_d6ae_51e0_9240_11786e164ce0_20230822005200_vrepl")
}
